@article{meyer_physicians_2013,
	title = {Physicians’ Diagnostic Accuracy, Confidence, and Resource Requests: A Vignette Study},
	volume = {173},
	issn = {2168-6106},
	doi = {10.1001/jamainternmed.2013.10081},
	shorttitle = {Physicians’ Diagnostic Accuracy, Confidence, and Resource Requests},
	abstract = {{\textless}h3{\textgreater}Importance{\textless}/h3{\textgreater}{\textless}p{\textgreater}Little is known about the relationship between physicians’ diagnostic accuracy and their confidence in that accuracy.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Objective{\textless}/h3{\textgreater}{\textless}p{\textgreater}To evaluate how physicians’\textit{diagnostic calibration}, defined as the relationship between diagnostic accuracy and confidence in that accuracy, changes with evolution of the diagnostic process and with increasing diagnostic difficulty of clinical case vignettes.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Design, Setting, and Participants{\textless}/h3{\textgreater}{\textless}p{\textgreater}We recruited general internists from an online physician community and asked them to diagnose 4 previously validated case vignettes of variable difficulty (2{\textless}i{\textgreater}easier{\textless}/i{\textgreater}; 2{\textless}i{\textgreater}more difficult{\textless}/i{\textgreater}). Cases were presented in a web-based format and divided into 4 sequential phases simulating diagnosis evolution: history, physical examination, general diagnostic testing data, and definitive diagnostic testing. After each phase, physicians recorded 1 to 3 differential diagnoses and corresponding judgments of confidence. Before being presented with definitive diagnostic data, physicians were asked to identify additional resources they would require to diagnose each case (ie, additional tests, second opinions, curbside consultations, referrals, and reference materials).{\textless}/p{\textgreater}{\textless}h3{\textgreater}Main Outcomes and Measures{\textless}/h3{\textgreater}{\textless}p{\textgreater}Diagnostic accuracy (scored as 0 or 1), confidence in diagnostic accuracy (on a scale of 0-10), diagnostic calibration, and whether additional resources were requested (no or yes).{\textless}/p{\textgreater}{\textless}h3{\textgreater}Results{\textless}/h3{\textgreater}{\textless}p{\textgreater}A total of 118 physicians with broad geographical representation within the United States correctly diagnosed 55.3\% of easier and 5.8\% of more difficult cases ({\textless}i{\textgreater}P{\textless}/i{\textgreater} \&lt; .001). Despite a large difference in diagnostic accuracy between easier and more difficult cases, the difference in confidence was relatively small (7.2 vs 6.4 out of 10, for easier and more difficult cases, respectively) ({\textless}i{\textgreater}P{\textless}/i{\textgreater} \&lt; .001) and likely clinically insignificant. Overall, diagnostic calibration was worse for more difficult cases ({\textless}i{\textgreater}P{\textless}/i{\textgreater} \&lt; .001) and characterized by overconfidence in accuracy. Higher confidence was related to decreased requests for additional diagnostic tests ({\textless}i{\textgreater}P{\textless}/i{\textgreater} = .01); higher case difficulty was related to more requests for additional reference materials ({\textless}i{\textgreater}P{\textless}/i{\textgreater} = .01).{\textless}/p{\textgreater}{\textless}h3{\textgreater}Conclusions and Relevance{\textless}/h3{\textgreater}{\textless}p{\textgreater}Our study suggests that physicians’ level of confidence may be relatively insensitive to both diagnostic accuracy and case difficulty. This mismatch might prevent physicians from reexamining difficult cases where their diagnosis may be incorrect.{\textless}/p{\textgreater}},
	pages = {1952--1958},
	number = {21},
	journaltitle = {{JAMA} Internal Medicine},
	shortjournal = {{JAMA} Intern Med},
	author = {Meyer, Ashley N. D. and Payne, Velma L. and Meeks, Derek W. and Rao, Radha and Singh, Hardeep},
	urldate = {2017-02-14},
	date = {2013-11-25},
	file = {Snapshot:C\:\\Users\\Trouble Maker\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\c3tv1gwb.default\\zotero\\storage\\CTEWVXZK\\1731967.html:text/html;Snapshot:C\:\\Users\\Trouble Maker\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\c3tv1gwb.default\\zotero\\storage\\2XNJHG8W\\1731967.html:text/html;Snapshot:C\:\\Users\\Trouble Maker\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\c3tv1gwb.default\\zotero\\storage\\I7DTKKAP\\1731967.html:text/html}
}


